{
  "name": "@blodless/common",
  "version": "1.0.5",
  "description": "MCP (Model Context Protocol) module for Nuxt",
  "type": "module",
  "main": "./dist/module.mjs",
  "types": "./dist/module.d.ts",
  "exports": {
    ".": {
      "types": "./dist/module.d.ts",
      "import": "./dist/module.mjs"
    },
    "./runtime": "./runtime/index.ts"
  },
  "scripts": {
    "build": "nuxt-module-build",
    "dev": "nuxt-module-build --stub && nuxt dev playground",
    "lint": "eslint --ext .ts,.js,.vue .",
    "lint:fix": "eslint --ext .ts,.js,.vue . --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "release": "npm run build && npm publish",
    "prepack": "npm run build"
  },
  "keywords": [
    "nuxt",
    "nuxt-module",
    "nuxt3",
    "nuxt4",
    "mcp",
    "model-context-protocol",
    "ai",
    "llm",
    "panel",
    "server-tools",
    "system-monitoring"
  ],
  "author": "blodless",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SoFunTy/nuxt-common-mcp.git",
    "directory": "modules/common"
  },
  "bugs": {
    "url": "https://github.com/SoFunTy/nuxt-common-mcp/issues"
  },
  "homepage": "https://github.com/SoFunTy/nuxt-common-mcp#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.2",
    "check-disk-space": "^3.4.0",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.6.0",
    "uuid": "^13.0.0",
    "zod": "^3.25.76"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3.12.0",
    "nuxt": "^3.0.0 || ^4.0.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.7.1",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "unbuild": "^2.0.0",
    "nuxt": "^4.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}